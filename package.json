  "name": "ketu",
  "version": "1.0.0",
  "description": "Ketu is an awesome steganography tool built purely on NodeJS.",
  "main": "main.js",
  "scripts": {  
    "start": "electron .",
    "uilib-start": "yarn watch:css && NODE_ENV=development webpack-dev-server",
    "build": "yarn build:css && NODE_ENV=production webpack --config ./webpack.config.js",
    "prebuild": "rm -rf ./dist/",
    "build:css": "postcss frontend/src/assets/tailwind.css -o frontend/src/assets/main.css",
    "watch:css": "postcss frontend/src/assets/tailwind.css -o frontend/src/assets/main.css"
  },
