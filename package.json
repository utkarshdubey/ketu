    "tailwindcss": "^1.8.10",
    "terser": "^5.3.1",
    "terser-webpack-plugin": "^4.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.0.0-beta.30",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
